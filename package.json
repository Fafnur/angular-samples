  "license": "MIT",
  "scripts": {
    "---- Module to Standalone ----": "",
    "start:mtc": "nx serve module-to-standalone",
    "build:mtc": "nx build module-to-standalone",
    "prerender:mtc": "nx run module-to-standalone:prerender:production && PROJECT=module-to-standalone ts-node minifier.ts",
    "ssr:mtc": "node dist/apps/module-to-standalone/server/ru/main.js",
    "extract-i18n:mtc": "nx extract-i18n module-to-standalone --out-file=apps/module-to-standalone/src/source.xlf",
    "api:mtc": "ts-node --project=apps/module-to-standalone/tsconfig.api.json apps/module-to-standalone/api-server.ts",
    "---- Icons ----": "",
    "start:icons": "nx serve icons",
    "build:icons": "nx build icons",
    "---- Icons Native ----": "",
    "start:icons:native": "nx serve icons-native",
    "build:icons:native": "nx build icons-native",
    "---- Redux ----": "",
    "start:redux": "nx serve redux-dashboard --devRemotes=redux-ngrx,redux-ngxs,redux-akita,redux-native",
    "build:redux": "nx build redux-dashboard && nx build redux-ngrx && nx build redux-ngxs && nx build redux-akita && nx build redux-native",
    "start:redux:dashboard": "nx serve redux-dashboard",
    "build:redux:dashboard": "nx build redux-dashboard",
    "start:redux:ngrx": "nx serve redux-ngrx",
    "build:redux:ngrx": "nx build redux-ngrx",
    "start:redux:ngxs": "nx serve redux-ngxs",
    "build:redux:ngxs": "nx build redux-ngxs",
    "start:redux:akita": "nx serve redux-akita",
    "start:build:akita": "nx build redux-akita",
    "start:redux:native": "nx serve redux-native",
    "start:build:native": "nx build redux-native",
    "---- Common ----": "",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "prepare": "husky install",
    "ng": "nx",
    "---- Other ----": "",
    "minify": "ts-node minifier.ts",
    "dev:ssr": "ng run module-to-standalone:serve-ssr",
    "serve:ssr": "node dist/module-to-standalone/server/main.js",
    "build:ssr": "ng build && ng run module-to-standalone:server",
    "prerender": "ng run module-to-standalone:prerender"
  },
